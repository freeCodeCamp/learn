  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.0.0",
    "@freecodecamp/curriculum": "^3.2.1",
    "adler32": "^0.1.7",
