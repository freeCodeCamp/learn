{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-function-bind",
    [
      "transform-imports", {
        "react-bootstrap": {
          "transform": "react-bootstrap/lib/${member}",
          "preventFullImport": true
        },
        "lodash": {
          "transform": "lodash/${member}",
          "preventFullImport": true
        }
      }
    ]
  ]
}